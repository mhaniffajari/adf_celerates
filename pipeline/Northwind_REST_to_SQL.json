{
	"name": "Northwind_REST_to_SQL",
	"properties": {
		"activities": [
			{
				"name": "REST_to_Blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"AbsoluteUrl": "$.$.__next"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "REST_Northwind",
						"type": "DatasetReference",
						"parameters": {
							"relative_url": "Customers"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "JSON_Blob_hanifajaristorageaccount",
						"type": "DatasetReference",
						"parameters": {
							"container": "adf",
							"folder": "stage",
							"file": "Customers.json"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Northwind"
		},
		"annotations": []
	}
}